################################################################################
# This file was generated by Ansible for {{ansible_fqdn}}
# Do NOT modify this file by hand!
################################################################################

user {{ nginx_user }} {{ nginx_group }};

worker_processes {{ nginx_worker_processes }};

worker_rlimit_nofile {{ nginx_worker_rlimit_nofile }};

{% if nginx_pid_file %}
pid {{ nginx_pid_file }};
{% endif %}

events {
{% for v in nginx_events_params %}
    {{ v }};
{% endfor %}
}

http {
{% for v in nginx_http_params %}
    {{ v if "}" in v[-2:] else v+";" }}
{% endfor %}

    # MIME
    include       {{ nginx_config_dir }}/mime.types;
    default_type  application/octet-stream;

{% for v in nginx_log_params %}
    {{ v if "}" in v[-2:] else v+";" }}
{% endfor %}

    include {{ nginx_snippets_dir }}/error-http.conf;

    include {{ nginx_config_dir }}/conf.d/*.conf;
    include {{ nginx_config_dir }}/sites-enabled/*;
}
